var gulp = require('gulp');
var acss = require('gulp-atomizer');

gulp.task('acss', () => {
  console.log('start');
  return gulp.src('./rw-gui-rt/**/*.tsx')
    .pipe(acss({
      // the filename of your output file. Default is `atomic.css`.
      outfile: 'test.css'
    }))
    .pipe(gulp.dest('rw-gui-rt'));
});